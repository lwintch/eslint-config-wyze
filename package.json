{
  "name": "eslint-config-wyze",
  "version": "5.0.0-beta.2",
  "description": "My personal ESLint config.",
  "license": "MIT",
  "repository": "wyze/eslint-config-wyze",
  "author": {
    "name": "Neil Kistner",
    "email": "neil.kistner@gmail.com",
    "url": "neilkistner.com"
  },
  "main": "index.js",
  "scripts": {
    "changelog": "changelog-maker --filter-release",
    "lint": "eslint rules *.js",
    "prelint": "npm link && npm link eslint-config-wyze",
    "pretest": "npm run lint",
    "test": "jest",
    "version": "node scripts/version.js"
  },
  "files": [
    "!**/__tests__/",
    "rules/*.js",
    "*.js"
  ],
  "babel": {
    "plugins": [
      "transform-inline-imports-commonjs"
    ]
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": "./base.js",
    "rules": {
      "import/default": 0,
      "import/named": 0,
      "no-bitwise": 0
    }
  },
  "peerDependencies": {
    "babel-eslint": "^7.1.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-wyze": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-inline-imports-commonjs": "^1.2.0",
    "changelog-maker": "^2.2.5",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-wyze": "^3.1.0",
    "execa": "^0.6.3",
    "jest": "^19.0.2",
    "read-pkg": "^2.0.0"
  }
}
