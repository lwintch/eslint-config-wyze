{
  "name": "eslint-config-lwintch-wyze",
  "version": "0.0.1-alpha.2",
  "description": "eslint config for sorting keys from wyze (github)",
  "license": "MIT",
  "repository": "git@github.com:lwintch/eslint-config-wyze.git",
  "author": {
    "name": "Neil Kistner",
    "email": "neil.kistner@gmail.com",
    "url": "neilkistner.com"
  },
  "main": "index.js",
  "scripts": {
    "boot": "yarn",
    "bump": "npm version --preid=alpha -m \"chore: publish %s\"",
    "lint": "eslint rules *.js",
    "pretest": "yarn lint",
    "test": "jest"
  },
  "files": [
    "!**/__tests__/",
    "rules/*.js",
    "*.js"
  ],
  "babel": {
    "plugins": [
      "transform-inline-imports-commonjs"
    ]
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": "./base.js",
    "rules": {
      "import/default": 0,
      "import/named": 0,
      "no-bitwise": 0
    }
  },
  "config": {
    "basePeers": {
      "eslint": "7.14.0",
      "eslint-config-airbnb": "18.2.1",
      "eslint-plugin-eslint-comments": "3.2.0",
      "eslint-plugin-import": "2.22.1",
      "eslint-plugin-lwintch-wyze": "0.0.1"
    },
    "reactPeers": {
      "eslint": "7.14.0",
      "eslint-config-airbnb": "18.2.1",
      "eslint-plugin-eslint-comments": "3.2.0",
      "eslint-plugin-import": "2.22.1",
      "eslint-plugin-jsx-a11y": "6.4.1",
      "eslint-plugin-react": "7.21.5",
      "eslint-plugin-lwintch-wyze": "0.0.1"
    },
    "flowPeers": {
      "babel-eslint": "10.1.0",
      "eslint": "7.14.0",
      "eslint-plugin-flowtype": "3.4.2"
    }
  },
  "peerDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.14.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-lwintch-wyze": "0.0.1"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-plugin-transform-inline-imports-commonjs": "1.2.0",
    "changelog-maker": "^2.2.5",
    "eslint": "7.14.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-lwintch-wyze": "0.0.1",
    "execa": "^0.6.3",
    "jest": "26.6.3",
    "read-pkg": "^2.0.0"
  }
}
